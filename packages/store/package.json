{
  "name": "@repo/store",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "tsc-watch -p tsconfig.json --onSuccess \"tsc-alias -p tsconfig.json\"",
    "build": "tsc && tsc-alias -p tsconfig.json"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/types/index.js"
    }
  },
  "devDependencies": {
    "tsc-alias": "^1.8.16",
    "tsc-watch": "^6.0.4",
    "typescript": "latest"
  },
  "dependencies": {
    "zod": "^4.3.6"
  }
}
